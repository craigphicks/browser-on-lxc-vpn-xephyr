  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Attach by Process ID",
      "processId": "${command:PickProcess}",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "default",
      "args":[ "create-ssh-config-lxc"],
      "program": "${workspaceFolder}/index.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "init firefox",
      "program": "${workspaceFolder}/index.js",
      "console": "internalConsole",
      "outputCapture": "std",
      "args":[ "post-init", "firefox"]
